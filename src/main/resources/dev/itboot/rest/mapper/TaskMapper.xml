<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.itboot.rest.mapper.TaskMapper">

<insert id="insert">
	INSERT INTO task(id,taskname) VALUES (#{id}, #{taskName})
</insert>

<delete id="truncate">
	DELETE FROM task WHERE user_name = #{userName};
</delete>


</mapper>